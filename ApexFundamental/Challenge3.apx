Integer num = 1;
do{
    //check if num is divisible by both 3 and 5
    if(Math.mod(num, 3) == 0 && Math.mod(num, 5) == 0){
        System.debug('FizzBuzz');
    }
    //Chek if the num is divisible by 3
    else if(Math.mod(num, 3) == 0){
        System.debug('Fizz');
    }
    //check if the num is divisible by 5
    else if(Math.mod(num, 5) == 0){
        System.debug('Buzz');
    }else{
        System.debug(num);
    }
    //increment value of num
    num++;
}while(num<=50);
